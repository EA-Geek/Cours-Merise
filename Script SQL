DROP TABLE IF EXISTS Eleve ;
CREATE TABLE Eleve (Id_Eleve BIGINT AUTO_INCREMENT NOT NULL,
Nom_Eleve VARCHAR(100),
Prenom_Eleve VARCHAR(100),
Date_naissance_Eleve DATE,
PRIMARY KEY (Id_Eleve)) ENGINE=InnoDB;

DROP TABLE IF EXISTS Devoir ;
CREATE TABLE Devoir (Id_Devoir BIGINT AUTO_INCREMENT NOT NULL,
Date_Entite 2 DATE,
Cours_Devoir VARCHAR(100),
PRIMARY KEY (Id_Devoir)) ENGINE=InnoDB;

DROP TABLE IF EXISTS Passer ;
CREATE TABLE Passer (Id_Eleve **NOT FOUND** AUTO_INCREMENT NOT NULL,
Id_Devoir **NOT FOUND** NOT NULL,
Note_Passer DECIMAL,
PRIMARY KEY (Id_Eleve,
 Id_Devoir)) ENGINE=InnoDB;

ALTER TABLE Passer ADD CONSTRAINT FK_Passer_Id_Eleve FOREIGN KEY (Id_Eleve) REFERENCES Eleve (Id_Eleve);

ALTER TABLE Passer ADD CONSTRAINT FK_Passer_Id_Devoir FOREIGN KEY (Id_Devoir) REFERENCES Devoir (Id_Devoir);
